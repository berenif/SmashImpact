{
  "name": "wasm-game-engine",
  "version": "1.0.0",
  "description": "Ultimate WASM Game Engine with advanced AI and physics",
  "main": "game.html",
  "scripts": {
    "build": "./build.sh",
    "build:docker": "./build.sh --docker",
    "build:quick": "./build-quick.sh",
    "build:production": "./build-quick.sh --production",
    "serve": "./build-quick.sh --serve",
    "test": "node tests/test-game.js && node tests/wolf-ai-tests.js",
    "test:wolf": "node tests/wolf-ai-tests.js",
    "test:game": "node tests/test-game.js",
    "check": "./build-quick.sh --check",
    "dev": "npm run build:quick && npm run serve",
    "clean": "rm -rf dist node_modules emsdk public/game_engine.js public/game_engine.wasm",
    "setup": "./build.sh --setup"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "game",
    "engine",
    "emscripten",
    "physics",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "uglify-js": "^3.17.4",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/wasm-game-engine.git"
  }
}